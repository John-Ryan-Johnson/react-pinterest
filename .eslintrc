{
  "parserOptions": {
    "ecmaVersion": 9,
    "sourceType": "module"
  },
  "extends": [
    "airbnb-base",
    "react-app",
    "plugin:prettier/recommended"
  ],
  "globals": {
    "document": true,
    "window": true,
    "$": true,
    "XMLHttpRequest": true,
    "allowTemplateLiterals": true
  },
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/prettier": "error"
    "no-console": [
      1,
      {
        "allow": [
          "error"
        ]
      }
    ],
    "no-debugger": 1,
    "class-methods-use-this": 0,
    "linebreak-style": 0,
    "max-len": [
      1,
      210,
      2
    ]
  }
}